{
   "name": "up-fetch",
   "version": "0.4.3",
   "description": "Slim and flexible HTTP client based on the Fetch API.",
   "type": "module",
   "main": "dist/index.cjs",
   "module": "dist/index.js",
   "types": "dist/index.d.ts",
   "sideEffects": false,
   "exports": {
      ".": {
         "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
         },
         "require": {
            "types": "./dist/index.d.cts",
            "default": "./dist/index.cjs"
         }
      },
      "./package.json": "./package.json"
   },
   "scripts": {
      "lint": "eslint . --ext .ts",
      "build": "tsup",
      "dev": "vitest",
      "test": "vitest run && tsc --noEmit",
      "prepublish": "npm run test && npm run build",
      "postpublish": "git add . && git commit -a && git tag v$npm_package_version && git push --tags && git push",
      "pub:beta": "npm publish --tag beta",
      "pub": "npm publish"
   },
   "keywords": [
      "fetch",
      "fetch wrapper",
      "request",
      "http",
      "config",
      "fetcher",
      "api",
      "rest",
      "get",
      "node-fetch",
      "axios",
      "wretch",
      "got",
      "ky"
   ],
   "author": "Laurent Blondy",
   "license": "ISC",
   "devDependencies": {
      "@typescript-eslint/eslint-plugin": "^5.57.0",
      "@typescript-eslint/parser": "^5.57.0",
      "eslint": "^8.37.0",
      "jsdom": "^21.1.1",
      "msw": "^2.2.3",
      "prettier": "^2.8.7",
      "tsup": "^8.0.2",
      "typescript": "^5.3.3",
      "valibot": ">=0.1.0",
      "vitest": "^0.29.8",
      "zod": ">=3"
   },
   "peerDependencies": {
      "zod": ">=3",
      "valibot": ">=0.1.0"
   },
   "peerDependenciesMeta": {
      "zod": {
         "optional": true
      },
      "valibot": {
         "optional": false
      }
   },
   "publishConfig": {
      "access": "public",
      "branches": [
         "master"
      ]
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/L-Blondy/up-fetch"
   },
   "engines": {
      "node": ">=18"
   }
}
