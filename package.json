{
   "name": "up-fetch",
   "version": "0.4.0",
   "description": "Slim and flexible HTTP client based on the Fetch API.",
   "type": "module",
   "main": "dist/index.js",
   "types": "dist/index.d.ts",
   "sideEffects": false,
   "scripts": {
      "lint": "eslint . --ext .ts",
      "build": "tsup",
      "dev": "vitest",
      "test": "vitest run && tsc --noEmit",
      "prepublish": "npm run test && npm run build",
      "postpublish": "git add . && git commit -a && git tag v$npm_package_version && git push --tags && git push"
   },
   "keywords": [
      "fetch",
      "fetch wrapper",
      "request",
      "http",
      "config",
      "fetcher",
      "api",
      "rest",
      "get",
      "node-fetch",
      "axios",
      "wretch",
      "got",
      "ky"
   ],
   "author": "Laurent Blondy",
   "license": "ISC",
   "devDependencies": {
      "@typescript-eslint/eslint-plugin": "^5.57.0",
      "@typescript-eslint/parser": "^5.57.0",
      "eslint": "^8.37.0",
      "jsdom": "^21.1.1",
      "msw": "^2.2.3",
      "node-fetch": "^3.3.1",
      "prettier": "^2.8.7",
      "tsup": "^8.0.2",
      "typescript": "^5.3.3",
      "valibot": ">=0.1.0",
      "vitest": "^0.29.8",
      "zod": ">=3"
   },
   "peerDependencies": {
      "zod": ">=3",
      "valibot": ">=0.1.0"
   },
   "peerDependenciesMeta": {
      "zod": {
         "optional": true
      },
      "valibot": {
         "optional": true
      }
   },
   "publishConfig": {
      "access": "public",
      "branches": [
         "master"
      ]
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/L-Blondy/up-fetch"
   },
   "engines": {
      "node": ">=18"
   }
}
